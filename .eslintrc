{
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "env": {
    "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "no-cond-assign": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-extra-semi": 2,
    "no-unreachable": 2,
    "curly": [
      2,
      "all"
    ],
    "dot-location": [
      2,
      "property"
    ],
    "dot-notation": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-with": 2,
    "wrap-iife": 2,
    "yoda": [
      2,
      "never"
    ],
    "no-unused-vars": [
      1,
      {
        "vars": "all",
        "args": "none"
      }
    ],
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "block-spacing": [
      2,
      "always"
    ],
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "computed-property-spacing": [
      2,
      "never"
    ],
    "consistent-this": [
      2,
      "_this"
    ],
    "eol-last": 2,
    "indent": [
      2,
      2,
      {
        "VariableDeclarator": {
          "var": 2
        },
        "SwitchCase": 1
      }
    ],
    "jsx-quotes": 2,
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "one-var": [
      2,
      {
        "var": "always",
        "let": "never",
        "const": "never"
      }
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "semi": [
      2,
      "always"
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "space-infix-ops": 2,
    "keyword-spacing": 2,
    "space-unary-ops": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/jsx-curly-spacing": 2,
    "react/jsx-no-undef": 2,
    "react/no-direct-mutation-state": 2,
    "react/no-unknown-property": 2,
    "react/self-closing-comp": 2
  },
  "plugins": [
    "react"
  ]
}
