{
  "name": "mdlr",
  "version": "0.0.3",
  "description": "a react component library for building declarative synthesizer instruments and systems",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo",
    "lint": "eslint .",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "pretty": "prettier --write --single-quote *.js src/**/*.js demo/**/*.js tests/**/*.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote",
      "git add"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "babel-eslint": "7.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "enzyme": "^2.7.1",
    "eslint": "3.8.1",
    "eslint-config-react-app": "^0.5.2",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "husky": "^0.13.2",
    "karma-chai-plugins": "^0.8.0",
    "lint-staged": "^3.3.1",
    "mocha": "^3.2.0",
    "note-to-frequency": "^1.4.1",
    "nwb": "0.15.x",
    "prettier": "^0.20.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "web-audio-test-api": "^0.5.2"
  },
  "author": "Noah Lemen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kedromelon/mdlr.git"
  },
  "keywords": [
    "react-component",
    "synthesizer",
    "synthesis",
    "music",
    "audio"
  ],
  "bugs": {
    "url": "https://github.com/kedromelon/mdlr/issues"
  },
  "homepage": "https://github.com/kedromelon/mdlr#readme"
}
